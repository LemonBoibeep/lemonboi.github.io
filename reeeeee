<!-- Begin
    function blinkit() {
        intrvl = 0;
        for (nTimes = 0; nTimes < 1000; nTimes++) {
            intrvl += 1000;
            setTimeout("document.bgColor='#';", intrvl);
            intrvl += 1000;
            setTimeout("document.bgColor='#FFFFFF';", intrvl);
        }
    }
// End -->
</script>

<body onload="blinkit(); return false;">
